#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://alasdairgray.github.io/ns/start/courseTitle
:courseTitle rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Course .


###  https://alasdairgray.github.io/ns/start/hasName
:hasName rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :Person .


###  https://alasdairgray.github.io/ns/start/hasStudent
:hasStudent rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :studies ;
            rdfs:domain :Course ;
            rdfs:range :Student .


###  https://alasdairgray.github.io/ns/start/hasTeacher
:hasTeacher rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :teaches ;
            rdfs:domain :Course ;
            rdfs:range :Academic .


###  https://alasdairgray.github.io/ns/start/studies
:studies rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :Student ;
         rdfs:range :Course .


###  https://alasdairgray.github.io/ns/start/teaches
:teaches rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :Academic ;
         rdfs:range :Course .


#################################################################
#    Classes
#################################################################

###  https://alasdairgray.github.io/ns/start/Academic
:Academic rdf:type owl:Class ;
          rdfs:subClassOf :Staff .


###  https://alasdairgray.github.io/ns/start/Administrative
:Administrative rdf:type owl:Class ;
                rdfs:subClassOf :Staff .


###  https://alasdairgray.github.io/ns/start/Course
:Course rdf:type owl:Class ;
        owl:disjointWith :Person .


###  https://alasdairgray.github.io/ns/start/Person
:Person rdf:type owl:Class .


###  https://alasdairgray.github.io/ns/start/Postgraduate
:Postgraduate rdf:type owl:Class ;
              rdfs:subClassOf :Student .


###  https://alasdairgray.github.io/ns/start/Staff
:Staff rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       rdfs:comment "A person who works at the university."@en ;
       rdfs:label "Staff member"@en .


###  https://alasdairgray.github.io/ns/start/Student
:Student rdf:type owl:Class ;
         rdfs:subClassOf :Person ;
         rdfs:comment "A person who studies at the university."@en ;
         rdfs:label "University student"@en .


###  https://alasdairgray.github.io/ns/start/Undergraduate
:Undergraduate rdf:type owl:Class ;
               rdfs:subClassOf :Student .


#################################################################
#    Individuals
#################################################################

###  https://alasdairgray.github.io/ns/start/alice
:alice rdf:type owl:NamedIndividual ,
                :Person ;
       :studies :f20bd .


###  https://alasdairgray.github.io/ns/start/f20bd
:f20bd rdf:type owl:NamedIndividual .


###  https://alasdairgray.github.io/ns/start/f21bd
:f21bd rdf:type owl:NamedIndividual ,
                :Course .


